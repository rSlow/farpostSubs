# AIOGRAM
BOT_TOKEN=12345:bot:token
WEBHOOK_SECRET=some-strong-sercet
BOT_MODE=WEBHOOK
DEBUG=true
TIMEZONE=America/Toronto

# HTTP
DOMAIN=your-domain.com
WEBHOOK_PATH=/webhook_path
WEBHOOK_PORT=443
WEBHOOK_ADDRESS=https://${DOMAIN}:${WEBHOOK_PORT}
WEB_SERVER_PORT=12345

# POSTGRES
POSTGRES_PORT=11111
POSTGRES_ADDRESS=localhost:${POSTGRES_PORT} # or docker container name in network
POSTGRES_USER=pg_user
POSTGRES_PASSWORD=pg_password
POSTGRES_DB=pg_db
DATABASE_URL=postgresql+asyncpg://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_ADDRESS}/${POSTGRES_DB}

# REDIS
REDIS_PORT=22222
REDIS_ADDRESS=localhost:${REDIS_PORT} # or docker container name in network
REDIS_PASS=redis_pass
REDIS_DB=0
REDIS_URL=redis://:${REDIS_PASS}@${REDIS_ADDRESS}/${REDIS_DB}

# RABBITMQ
RABBITMQ_PANEL_PORT=33333
RABBITMQ_PORT=44444
RABBITMQ_ADDRESS=localhost:${RABBITMQ_PORT} # or docker container name in network
RABBITMQ_DEFAULT_USER=rabbitmq_user
RABBITMQ_DEFAULT_PASS=rabbitmq_pass
RABBITMQ_URL=amqp://${RABBITMQ_DEFAULT_USER}:${RABBITMQ_DEFAULT_PASS}@${RABBITMQ_ADDRESS}

# SELENIUM
SELENIUM_PORT=55555
SELENIUM_ADDRESS=localhost:${SELENIUM_PORT} # or docker container name in network
SELENIUM_URL=http://${SELENIUM_ADDRESS}/wd/hub